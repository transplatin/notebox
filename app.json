{
  "expo": {
    "name": "NoteBox",
    "slug": "NoteBox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.notebox.app",
      "versionCode": 1,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5315357128356077~5556035357" // sample id, replace with your own
      }
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5315357128356077~5556035357" // sample id, replace with your own
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
